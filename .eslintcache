 {\n\t\tif (filter === name) {\n\t\t\tsetFilter(\"\");\n\t\t} else {\n\t\t\tsetFilter(name);\n\t\t}\n\t}\n\n\tconst handleCheck = (evt) => {\n\t\tconst { name, checked } = evt.target;\n\t\tsetActiveFilters(name);\n\t}\n\n\treturn (\n\t\t<>\n\t\t\t<OptionsContainer>\n\t\t\t\t{\n\t\t\t\t\tObject.entries(search.facets).map(([k, v]) => {\n\t\t\t\t\t\treturn <div key={k} onClick={() => handleOptionClick(k)}>{k}</div>\n\t\t\t\t\t})\n\t\t\t\t}\n\t\t\t\t<div onClick={() => handleOptionClick(\"one\")}>Option 1</div>\n\t\t\t\t<div onClick={() => handleOptionClick(\"two\")}>Option 2</div>\n\t\t\t</OptionsContainer>\n\n\t\t\t{\n\t\t\t\tObject.entries(search.facets).map(([k, v]) => {\n\t\t\t\t\tif (v) {\n\t\t\t\t\t\treturn (\n\t\t\t\t\t\t\t<SCheckContainer shown={filter === k}>\n\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\t\t\t\tObject.entries(v).map(([k2, v2]) => {\n\t\t\t\t\t\t\t\t\t\treturn (\n\t\t\t\t\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t\t\t\t\t<input name={k2} id={k2} type=\"checkbox\" key={k2} checked={activeFilters.includes(k2)} onChange={handleCheck} />\n\t\t\t\t\t\t\t\t\t\t\t\t<label htmlFor={k2}>{k2}</label>\n\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\t\t\t})\n\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t</SCheckContainer>\n\t\t\t\t\t\t)\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t}\n\n\t\t\t<SCheckContainer shown={filter === \"one\"}>\n\t\t\t\t<input name=\"type-yoga\" type=\"checkbox\" filter=\"type\" key=\"typeBox1\" checked={activeFilters.includes(\"yoga\")} onChange={handleCheck} />\n\t\t\t\t<label for=\"type-yoga\">yoga</label>\n\t\t\t</SCheckContainer>\n\t\t\t<SCheckContainer shown={filter === \"two\"}>\n\t\t\t\t<input name=\"type-cardio\" type=\"checkbox\" key=\"typeBox4\" checked={activeFilters.includes(\"cardio\")} onChange={handleCheck} />\n\t\t\t\t<label for=\"type-cardio\">cardio</label>\n\t\t\t\t<input name=\"type-stretch\" type=\"checkbox\" key=\"typeBox5\" checked={activeFilters.includes(\"stretch\")} onChange={handleCheck} />\n\t\t\t\t<label for=\"type-stretch\">stretch</label>\n\t\t\t</SCheckContainer>\n\n\t\t</>\n\t)\n}\n\n\nconst SCheckContainer = styled.div`\n\tdisplay: ${({ shown }) => shown ? \"flex\" : \"none\"};\n\tflex-flow: column nowrap;\n\twidth: 50rem;\n\tjustify-content: center;\n\talign-items: center;\n\tinput, label {\n\t\tfont-size: 2rem;\n\t\tfont-family: Raleway;\n\t}\n`;\n\nconst OptionsContainer = styled.div`\n\tdisplay: flex;\n\tflex-flow: row nowrap;\n\talign-items: center;\n\theight: 60px;\n\tdiv {\n\t\tdisplay: block;\n\t\twidth: 20rem;\n\t\tmargin: 2rem;\n\t\tborder-radius: 50px;\n\t\tborder: 2px solid black;\n\t\tpadding: 1rem;\n\t\ttext-align: center;\n\t\tfont-family: Raleway;\n\t\ttext-align: center;\n\t\tfont-size: 1.8rem;\n\t\tfont-weight: 700;\n\t}\n`;\n\nexport default CheckboxGroup;","/Users/chazadmin/code/projects/lambda/bw4/frontend-cookbook/src/pages/search/components/Checkbox.jsx",["67"],"/Users/chazadmin/code/projects/lambda/bw4/frontend-cookbook/src/pages/search/SearchContainer.jsx",[],"/Users/chazadmin/code/projects/lambda/bw4/frontend-cookbook/src/common/components/Header.jsx",[],"/Users/chazadmin/code/projects/lambda/bw4/frontend-cookbook/src/store/rootReducer.js",[],"/Users/chazadmin/code/projects/lambda/bw4/frontend-cookbook/src/pages/search/slice/initState.js",[],"/Users/chazadmin/code/projects/lambda/bw4/frontend-cookbook/src/pages/search/slice/searchSlice.js",[],{"ruleId":"68","severity":1,"message":"69","line":29,"column":21,"nodeType":"70","messageId":"71","endLine":29,"endColumn":34},{"ruleId":"68","severity":1,"message":"72","line":2,"column":23,"nodeType":"70","messageId":"71","endLine":2,"endColumn":34},{"ruleId":"68","severity":1,"message":"73","line":30,"column":17,"nodeType":"70","messageId":"71","endLine":30,"endColumn":24},{"ruleId":"74","severity":1,"message":"75","line":47,"column":48,"nodeType":"76","messageId":"77","endLine":47,"endColumn":50},{"ruleId":"68","severity":1,"message":"78","line":6,"column":8,"nodeType":"70","messageId":"71","endLine":6,"endColumn":19}